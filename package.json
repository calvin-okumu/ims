{
  "name": "ims",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:network": "next dev -H 0.0.0.0",
    "dev:port80": "sudo ./start-port80.sh",
    "bridge:install": "./install-bridge.sh",
    "bridge:install:windows": "install-bridge-windows.bat",
    "bridge:start": "./start-bridge.sh",
    "bridge:start:windows": "start-bridge.bat",
    "bridge:test": "python3 services/fingerprint_bridge_simple.py",
    "bridge:test:windows": "python services/fingerprint_bridge_windows.py",
    "firewall:disable": "sudo ufw disable",
    "firewall:enable": "sudo ufw enable && sudo ufw allow 3000",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "prepare": "husky"
  },
  "dependencies": {
    "@sentry/nextjs": "^10.32.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/uuid": "^10.0.0",
    "axios": "^1.13.2",
    "axios-retry": "^4.5.0",
    "better-sqlite3": "^12.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "lucide-react": "^0.559.0",
    "next": "16.0.8",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "swr": "^2.3.7",
    "tailwind-merge": "^3.4.0",
    "uuid": "^13.0.0",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.8",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.7",
    "msw": "^2.12.4",
    "prettier": "^3.7.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
